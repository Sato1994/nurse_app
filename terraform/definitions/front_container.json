[
  {
    "name": "front_container",
    "image": "850929304034.dkr.ecr.ap-northeast-1.amazonaws.com/front:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "front",
        "awslogs-group": "/ecs/front"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8080
      }
    ],
    "secrets": [
      {
        "name": "BROWSER_BASE_URL",
        "valueFrom": "BROWSER_BASE_URL"
      },
      {
        "name": "BASE_URL",
        "valueFrom": "BASE_URL"
      }
    ],
    "command": [
      "npm",
      "run",
      "start"
    ]
  }
]